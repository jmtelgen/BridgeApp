# Use nx for monorepo
npx nx list @nx/react
npx nx g @nx/react:component --directory=src/app/hello-world hello-world
npx nx build
npm add {dependency}
nx graph
nx run-many -t lint
# Create local libraries
nx g @nx/react:library products --unitTestRunner=vitest --bundler=none --directory=modules/products
# Import local libraries
nx g @nx/react:component product-list --directory=modules/products/src/lib/product-list
